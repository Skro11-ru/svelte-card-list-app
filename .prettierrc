{
  "arrowParens": "always",
  "singleQuote": true,
  "printWidth": 80,
  "bracketSpacing": true,
  "tabWidth": 2,
  "plugins": ["prettier-plugin-svelte"],
  "semi": true,
  "svelteSortOrder": "options-markup-scripts-styles",
  "svelteStrictMode": true,
  "svelteBracketNewLine": true,
  "svelteIndentScriptAndStyle": true,
  "trailingComma": "all"
}
